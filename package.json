{
  "name": "pokeping",
  "version": "0.1.0",
  "description": "Locates nearby Pokemon",
  "main": "pokesearch.js",
  "scripts": {
    "test": "standard",
    "start": "concurrently 'npm run start-web' 'npm run start-server'",
    "start-dev": "concurrently 'npm run start-web' 'node test/test-server.js'",
    "start-web": "concurrently 'npm run watch-js' 'http-server -p 8081'",
    "start-server": "node server.js",
    "build-js": "browserify app/main.js -t [ babelify --presets es2015 ] -g uglifyify -o dist/bundle.js",
    "watch-js": "watchify app/main.js -t [ babelify --presets es2015 ] -o dist/bundle.js"
  },
  "author": "rcrichton",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "angular2-in-memory-web-api": "0.0.14",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "cors": "^2.7.1",
    "es6-promise": "^3.2.1",
    "express": "^4.14.0",
    "geolib": "^2.0.21",
    "moment": "^2.14.1",
    "pokemongo-protobuf": "^1.6.0",
    "reflect-metadata": "0.1.3",
    "request": "^2.73.0",
    "rxjs": "5.0.0-beta.6",
    "s2geometry-node": "^1.3.1",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "concurrently": "^2.2.0",
    "http-server": "^0.9.0",
    "node-sass": "^3.8.0",
    "standard": "^7.1.2",
    "uglifyify": "^3.0.2",
    "watchify": "^3.7.0"
  }
}
